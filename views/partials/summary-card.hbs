<div class="col-md-4">
    <div class="card mb-4 shadow-sm">
        <div width="100%" height="225" class="bd-placeholder-img card-img-top">
            <div id={{createUniqueId "myCarousel" location.zip_code}} class="carousel slide" data-ride="carousel"
                style="margin-bottom: 0" data-interval="false">
                <ol class="carousel-indicators">
                    <li style="background-color: #007bff;"
                        data-target={{createUniqueId "#myCarousel" location.zip_code}} data-slide-to="0" class="active">
                    </li>
                    </li>
                    <li id={{createUniqueId "listItemMap" location.zip_code}} style="background-color: #007bff;"
                        data-target={{createUniqueId "#myCarousel" location.zip_code}} data-slide-to="1">
                    </li>
                    <li style="background-color: #007bff;"
                        data-target={{createUniqueId "#myCarousel" location.zip_code}} data-slide-to="2">
                    </li>
                </ol>
                <div class="carousel-inner">
                    <div class="carousel-item active">
                        {{{graphics}}}
                    </div>
                    {{!-- <div class="carousel-item">
                        {typeof window !== 'undefined' &&
                        <LeafletMap position={[item.latitude, item.longitude]} zoom={9} markers={markers}
                            key={keyMap} />
                        }
                        <p style={{ textAlign: "center", marginTop: "1rem" }}>Covid-19 Testing Locations</p>
                        {/* <div class="container">
                            <div class="carousel-caption text-center">
                                <p>Covid-19 Testing Locations</p>
                            </div>
                        </div> */}
                    </div> --}}

                    <div class="carousel-item">
                        <img class="third-slide"
                            src="data:image/gif;base64,R0lGODlhAQABAIAAAHd3dwAAACH5BAAAAAAALAAAAAABAAEAAAICRAEAOw=="
                            alt="Third slide" />
                        <div class="container">
                            <div class="carousel-caption">
                                <p>{{location.state}} Department of Health</p>
                                <ul>
                                    <li><a class="covid19-links" href={{location.statesInfo.covid19Site}}>Covid19
                                            Site</a></li>
                                    <li><a class="covid19-links"
                                            href={{location.statesInfo.covid19SiteSecondary}}>Covid19 Secondary
                                            Site</a></li>
                                    {{{getTwitter location.statesInfo.twitter}}}
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="card-body">
            <h5 class="card-text">{{location.alias}}</h5>
            <div class="d-flex justify-content-between align-items-center">
                <div class="btn-group">
                    {{!-- <button class="btn btn-sm btn-secondary">View details</button> --}}
                </div>
                <small class="text-muted"></small>
            </div>
        </div>
    </div>
</div>